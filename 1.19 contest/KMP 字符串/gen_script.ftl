<#-- 小数据，只包含 ab 的字符串 -->
gen -n 5 -m 10 -type 1 > $
gen -n 10 -m 20 -type 1 > $

<#-- 中等数据，只包含小写字母 -->
gen -n 100 -m 1000 -type 2 > $
gen -n 1000 -m 10000 -type 2 > $

<#-- 大数据，包含所有可能的字符 -->
gen -n 10000 -m 100000 -type 3 > $
gen -n 50000 -m 500000 -type 3 > $

<#-- 极限数据 -->
gen -n 100000 -m 1000000 -type 1 > $
gen -n 100000 -m 1000000 -type 2 > $
gen -n 100000 -m 1000000 -type 3 > $
gen -n 1000 -m 1000000 -type 3 > $ 